  "test_count": 3,
  "tests": [
    {
      "name": "Load portfolio homepage",
      "description": "Verify that the portfolio homepage loads successfully",
      "page_url": "https://tc960.github.io/",
      "steps": [
        {
          "action": "goto",
          "url": "https://tc960.github.io/"
        },
        {
          "action": "expect",
          "selector": "body",
          "assertion": "toBeVisible"
        },
        {
          "action": "expect",
          "selector": "title",
          "assertion": "toContainText",
          "text": "Mohak Akul Prakash | Data Science & AI Portfolio"
        }
      ]
    },
    {
      "name": "Check page metadata",
      "description": "Verify that the page has proper metadata and title",
      "page_url": "https://tc960.github.io/",
      "steps": [
        {
          "action": "goto",
          "url": "https://tc960.github.io/"
        },
        {
          "action": "expect",
          "selector": "head meta[name='viewport']",
          "assertion": "toBeVisible"
        },
        {
          "action": "expect",
          "selector": "head meta[charset='UTF-8']",
          "assertion": "toBeVisible"
        }
      ]
    },
    {
      "name": "Verify page loads with no errors",
      "description": "Check that the page loads without any console errors",
      "page_url": "https://tc960.github.io/",
      "steps": [
        {
          "action": "goto",
          "url": "https://tc960.github.io/"
        },
        {
          "action": "expect",
          "selector": "body",
          "assertion": "not.toContainText",
          "text": "Error"
        }
      ]
    }
  ]
}
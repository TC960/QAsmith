  "tests": [
    {
      "name": "Industry Solutions Page Navigation",
      "description": "Tests basic navigation and link functionality on the Industry Solutions page",
      "page_url": "https://github.com/solutions/industry",
      "steps": [
        {
          "action": "goto",
          "url": "https://github.com/solutions/industry"
        },
        {
          "action": "expect",
          "selector": "h1",
          "assertion": "toBeVisible"
        },
        {
          "action": "click",
          "selector": "a:text('Sign in')"
        },
        {
          "action": "expect",
          "selector": "#login",
          "assertion": "toBeVisible"
        },
        {
          "action": "goto",
          "url": "https://github.com/solutions/industry"
        },
        {
          "action": "click",
          "selector": "a:text('GitHub Copilot')"
        },
        {
          "action": "expect",
          "selector": "text=GitHub Copilot",
          "assertion": "toBeVisible"
        }
      ]
    },
    {
      "name": "Login Page Functionality",
      "description": "Tests login form functionality and error handling",
      "page_url": "https://github.com/login",
      "steps": [
        {
          "action": "goto",
          "url": "https://github.com/login"
        },
        {
          "action": "expect",
          "selector": "#login",
          "assertion": "toBeVisible"
        },
        {
          "action": "fill",
          "selector": "input[name='login']",
          "value": "testuser"
        },
        {
          "action": "fill",
          "selector": "input[name='password']",
          "value": "wrongpassword"
        },
        {
          "action": "click",
          "selector": "input[type='submit']"
        },
        {
          "action": "expect",
          "selector": ".flash-error",
          "assertion": "toBeVisible"
        },
        {
          "action": "click",
          "selector": "a:text('Forgot password?')"
        },
        {
          "action": "expect",
          "selector": "text=Reset your password",
          "assertion": "toBeVisible"
        }
      ]
    },
    {
